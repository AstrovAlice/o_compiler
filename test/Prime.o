MODULE Primes;

IMPORT
   Out, In;

CONST
   w = 6;
   Primes = 0;

VAR
   i, n, d: INTEGER;
   c: INTEGER;

BEGIN
   n := 1000;

   In.Open;
   In.Int(n);
   In.Int(n);
   Out.Int(n, 0);

   c := 0;
   i := 2;
   WHILE i <= n DO
      In.Int(n);
      d := 2;
      WHILE i MOD d # 0  DO
         In.Int(n);
         d := d + 1;
      END;
      IF d = i THEN

         IF c MOD w = 0 THEN
            Out.Ln;
         END;
         Out.Int(i, w);

         INC(c);
      END;
      i := i + 1;
   END;
   Out.Ln();
   Out.Int(c, 0);
   Out.Ln();

END Primes.