MODULE Primes
import
   Out, In
const
   w = 6
var
   n, c int
begin
   In.open
   In.int(n)
   c := 0
   i := 2
   while i <= n do
      d := 2
      d := 2
      while i mod d # 0 do
          d := d + 1
      end
      if d = i then
         Out.int(i, 0)
         Out.ln
         inc(c)
      end
      i := i + 1
   end
   Out.int(c, 0)
   Out.ln()
end Primes.